      <option value="Corsair SF 600W">Corsair SF 600W</option>
      <option value="Corsair SF 450W">Corsair SF 450W</option>
    </select>
    <br>
    <label for="ram-select">RAM:</label>
    <select class="ram" name="ram" id="ram-select">
      <option value="GSkill Trident Z5 32GB-6000Mhz">G.Skill Trident Z5 *RGB* DDR5-6000Mhz (2 x 16GB)</option>
      <option value="Samsung DDR5 4800 32GB-4800Mhz">Samsung DDR5-4800Mhz (2 x 16GB)</option>
      <option value="TeamGroup T-Force Xtreem 16GB-3600Mhz">TeamGroup T-Force Xtreem DDR4-3600Mhz (2 x 8GB)</option>
      <option value="Patriot Viper Steel 16GB-4400Mhz">Patriot Viper Steel DDR4-4400Mhz (2 x 8GB)</option>
      <option value="Patriot Viper RGB 16GB-3600Mhz">Patriot Viper *RGB* DDR4-3600Mhz (2 x 8GB)</option>
      <option value="Patriot Viper 4 16GB-3400Mhz">Patriot Viper 4 DDR4-3400Mhz (2 x 8GB)</option>
      <option value="Corsair V RGB 32GB-3200Mhz">Corsair Vengence *RGB* Pro DDR4-3200Mhz (4 x 8GB)</option>
    </select>
    <br>
    <button class="add" type="submit">Add Finalized PC</button>
  </div>
  </form>
</main>

<%- include('../partials/footer') %>